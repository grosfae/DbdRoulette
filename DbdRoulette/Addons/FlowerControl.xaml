<UserControl x:Class="DbdRoulette.Addons.FlowerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DbdRoulette.Addons"
             mc:Ignorable="d" 
             Height="300" Width="570" Loaded="UserControl_Loaded">
    <Grid>
        <ToggleButton x:Name="MainToggle" Width="160" Height="160" Style="{DynamicResource FogToggleButton}" BorderThickness="0" Checked="MainToggle_Checked" Unchecked="MainToggle_Unchecked"/>
    </Grid>
</UserControl>
